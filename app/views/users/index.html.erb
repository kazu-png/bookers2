<%= render 'users/info', user: @user %>
<%= render 'books/new', book: @book %>
<h2>Users</h2>
   <table>
     <thead>
      <tr>
         <th>image</th>
         <th>name</th>
         <th></th>
      </tr>
     </thead>
     <tbody>
      <% @users.each do |user| %>
       <tr>
        <td><%= attachment_image_tag user, :profile_image%></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
       </tr>
      <% end %>
     </tbody>
    </table>